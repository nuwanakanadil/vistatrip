<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee
         http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd"
         version="3.1">



     <servlet>
     	<servlet-name>NewTourGuideInsertServlet</servlet-name>
     	<servlet-class>TourGuide.NewTourGuideInsertServlet</servlet-class>
     </servlet>
     
     <servlet-mapping>
     	<servlet-name>NewTourGuideInsertServlet</servlet-name>
     	<url-pattern>/NewTourGuideInsertServlet</url-pattern>
     </servlet-mapping>
     
     
        <servlet>
     	<servlet-name>TourGuideLoginServlet</servlet-name>
     	<servlet-class>TourGuide.TourGuideLoginServlet</servlet-class>
     </servlet>
     
     <servlet-mapping>
     	<servlet-name>TourGuideLoginServlet</servlet-name>
     	<url-pattern>/TourGuideLoginServlet</url-pattern>
     </servlet-mapping>
     
        <servlet>
     	<servlet-name>GetAllTourGuides</servlet-name>
     	<servlet-class>TourGuide.GetAllTourGuides</servlet-class>
     </servlet>
     
     <servlet-mapping>
     	<servlet-name>GetAllTourGuides</servlet-name>
     	<url-pattern>/GetAllTourGuides</url-pattern>
     </servlet-mapping>


       <servlet>
     	<servlet-name>TourGuideUpdateServlet</servlet-name>
     	<servlet-class>TourGuide.TourGuideUpdateServlet</servlet-class>
     </servlet>
     
     <servlet-mapping>
     	<servlet-name>TourGuideUpdateServlet</servlet-name>
     	<url-pattern>/TourGuideUpdateServlet</url-pattern>
     </servlet-mapping>
     
     
      <servlet>
     	<servlet-name>TourGuideDeleteServlet</servlet-name>
     	<servlet-class>TourGuide.TourGuideDeleteServlet</servlet-class>
     </servlet>
     
     <servlet-mapping>
     	<servlet-name>TourGuideDeleteServlet</servlet-name>
     	<url-pattern>/TourGuideDeleteServlet</url-pattern>
     </servlet-mapping>
     

</web-app>